{
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prefer-stateless-function": 0
  },
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "fetch": true
  }
}